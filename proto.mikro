object Math
    static function quadratic(a : number, b : number, c : number) -> void
        var radical = pow(b, 2) - (4 * a * c)

        if(radical >= 0)
            var diff_top = (0 - pow(b, 2)) - sqrt(radical)
            var plus_top = (0 - pow(b, 2)) + sqrt(radical)
            var bottom = 2 * a

            print(plus_top / bottom)
            print(diff_top / bottom)
        else
            print("Imaginary roots!")
        end
    end
end

Math.quadratic(1, 6, 5)

